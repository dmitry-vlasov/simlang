export {

Exp ::= Let, Assign, If, While, Seq, Call, Array, Print, Read, Var, Int, Bool, String;
	Let(var : string, e: Exp, body: Exp);
	Assign(var : string, e: Exp);
	If(cond: Exp, pos: Exp, neg: Exp);
	While(cond: Exp, body: Exp);
	Seq(stats: [Exp]);
	Print(e: Exp);
	Read(var: string);

	Call(op : string, args : [Exp]);
	Array(vals : [Exp]);
	Var(v: string);
	Int(i : int);
	Bool(b: bool);
	String(s : string);
}
